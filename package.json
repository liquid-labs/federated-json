{
  "name": "@liquid-labs/federated-json",
  "version": "1.0.0-alpha.27",
  "description": "Library that builds a single JSON object from multiple JSON files.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "lint": "JS_SRC=src/fjson catalyst-scripts lint",
    "lint:fix": "JS_SRC=src/fjson catalyst-scripts lint-fix",
    "build": "JS_SRC=src/fjson catalyst-scripts build && bash-rollup src/cli/fjson.sh dist/fjson.sh",
    "build:example": "JS_FILE=src/example/example.js JS_OUT=./example.js catalyst-scripts build",
    "example": "npm run build:example && node ./example.js",
    "prepare": "npm run build",
    "pretest": "JS_SRC=src/fjson catalyst-scripts pretest && mkdir -p test-staging/test/data/datadir && cp src/fjson/test/data/*.json test-staging/test/data && cp src/fjson/test/data/datadir/*.json test-staging/test/data/datadir",
    "test": "JS_SRC=src/fjson catalyst-scripts test",
    "preversion": "JS_SRC=src npm run lint && npm test",
    "postversion": "git push upstream v${npm_package_version}"
  },
  "bin": {
    "fjson": "dist/fjson.sh"
  },
  "files": [
    "dist/"
  ],
  "keywords": [],
  "author": "Zane Rockenbaugh <zane@liquid-labs.com>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Liquid-Labs/federated-json.git"
  },
  "bugs": {
    "url": "https://github.com/Liquid-Labs/federated-json/issues"
  },
  "homepage": "https://github.com/Liquid-Labs/federated-json#readme",
  "liq": {
    "packageType": "node|lib|cli",
    "orgBase": "git@github.com:Liquid-Labs/Liquid-Labs.git",
    "eslint": {
      "target": "./src/fjson"
    }
  },
  "devDependencies": {
    "@liquid-labs/bash-rollup": "^1.0.0-alpha.6",
    "@liquid-labs/bash-toolkit": "^1.0.0-alpha.57",
    "@liquid-labs/catalyst-scripts": "^1.0.0-alpha.57"
  }
}
